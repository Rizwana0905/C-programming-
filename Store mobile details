#include <stdio.h>

struct Mobile
{
    int mobileId;
    char brand[30];
    char model[30];
    float price;
    int quantity;
};

int main()
{
    struct Mobile m;

    printf("Enter Mobile ID: ");
    scanf("%d", &m.mobileId);

    printf("Enter Brand Name: ");
    scanf(" %[^\n]", m.brand);

    printf("Enter Model Name: ");
    scanf(" %[^\n]", m.model);

    printf("Enter Price: ");
    scanf("%f", &m.price);

    printf("Enter Quantity: ");
    scanf("%d", &m.quantity);

    printf("\n--- Mobile Details ---\n");
    printf("Mobile ID : %d\n", m.mobileId);
    printf("Brand     : %s\n", m.brand);
    printf("Model     : %s\n", m.model);
    printf("Price     : %.2f\n", m.price);
    printf("Quantity  : %d\n", m.quantity);

    return 0;
}
